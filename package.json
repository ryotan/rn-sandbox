{
  "name": "rn-sandbox",
  "version": "0.1.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "pod-install": "bundle exec pod install --repo-update --project-directory=ios",
    "pod-install:clean": "run-s clean:pod pod-install",
    "prebuild": "run-s prebuild:no-install prebuild:npm-install pod-install",
    "prebuild:no-install": "expo prebuild --no-install",
    "prebuild:npm-install": "npm install",
    "prebuild:clean": "rimraf android ios && run-s prebuild",
    "test": "jest",
    "test:coverage": "run-s 'test -- --coverage {@}' --",
    "test:report": "run-s 'test:coverage -- --reporters=default --reporters=jest-junit {@}' --",
    "test:ci": "run-s 'test:report -- --ci --forceExit {@}' --",
    "test:debug": "run-s 'test -- --forceExit --detectOpenHandles --runInBand {@}' --",
    "lint": "run-p --print-name --continue-on-error lint:*",
    "lint:es": "eslint --ignore-path .gitignore --ext .jsx,.js,.tsx,.ts .",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-s --print-name --continue-on-error fix:*",
    "fix:es": "eslint --ignore-path .gitignore --fix --ext .jsx,.js,.tsx,.ts .",
    "clean-all": "run-p clean:*",
    "clean:build:android": "rimraf android/app/build",
    "clean:build:ios": "rimraf ios/build",
    "clean:bundle": "rimraf .bundle/vendor",
    "clean:jest": "rimraf __jest__/reports",
    "clean:pod": "rimraf ios/Pods",
    "barrels": "run-s barrels:clean barrels:generate",
    "barrels:clean": "rimraf -v --glob -- 'src/bases/**/index.ts'",
    "barrels:generate": "npx barrelsby -c .barrelsby.json",
    "bundle-install": "bundle install",
    "orval": "npx orval --config ./orval.config.ts",
    "setup:mac": "npm run bundle-install && npm clean-install && npm run pod-install",
    "setup:mac:clean": "run-s clean-all setup:mac",
    "postinstall": "run-s postinstall:*",
    "postinstall:patch-package": "patch-package",
    "prepare": "husky install"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.0",
    "@material/material-color-utilities": "^0.2.4",
    "@mswjs/data": "^0.13.0",
    "@react-native-community/netinfo": "9.3.7",
    "@react-native-community/slider": "4.4.2",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/core": "^6.4.8",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/elements": "^1.3.17",
    "@react-navigation/material-bottom-tabs": "^6.2.15",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@shopify/restyle": "^2.4.2",
    "@tanstack/react-query": "^4.28.0",
    "axios": "^1.3.4",
    "expo": "~48.0.11",
    "expo-application": "~5.1.1",
    "expo-asset": "~8.9.1",
    "expo-blur": "~12.2.2",
    "expo-constants": "~14.2.1",
    "expo-crypto": "~12.2.1",
    "expo-device": "~5.2.1",
    "expo-haptics": "~12.2.1",
    "expo-image": "~1.0.0",
    "expo-keep-awake": "~12.0.1",
    "expo-linear-gradient": "~12.1.2",
    "expo-linking": "~4.0.1",
    "expo-localization": "~14.1.1",
    "expo-screen-orientation": "~5.1.1",
    "expo-secure-store": "~12.1.1",
    "expo-splash-screen": "~0.18.1",
    "expo-sqlite": "~11.1.1",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "formik": "^2.2.9",
    "fp-ts": "^2.13.1",
    "lodash": "^4.17.21",
    "msw": "^1.2.1",
    "nanoid": "^4.0.2",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-paper": "^5.5.2",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-url-polyfill": "^1.3.0",
    "ts-pattern": "^4.2.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tanstack/eslint-plugin-query": "^4.28.1",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.0.1",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.0.14",
    "@types/react-test-renderer": "~18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "barrelsby": "^2.5.1",
    "cross-env": "^7.0.3",
    "eslint-config-universe": "^11.2.0",
    "eslint-formatter-rdjson": "^1.0.5",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-strict-dependencies": "^1.1.0",
    "expo-build-properties": "~0.5.1",
    "expo-dev-client": "~2.1.6",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "jest": "^29.2.1",
    "jest-expo": "^48.0.2",
    "jest-junit": "^16.0.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "orval": "^6.14.4",
    "patch-package": "^7.0.0",
    "prettier": "^2.8.7",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^5.0.0",
    "ts-expect": "^1.3.0",
    "typescript": "^4.9.4"
  },
  "jest-junit": {
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}",
    "ancestorSeparator": " â€º ",
    "outputDirectory": "__jest__/reports/",
    "outputName": "jest-junit.xml"
  },
  "private": true
}
